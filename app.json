{
  "expo": {
    "name": "Cooky",
    "slug": "Cooky",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cooky.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash":{
      "image" :"./assets/images/cooky.png",
      "imageWidth": 100,
      "resizeMode" :"contain",
      "backgroundColor": "#1B3C3D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cooky.png",
        "backgroundColor": "#1B3C3D"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/cooky.png"
    },
    "plugins": [

    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
